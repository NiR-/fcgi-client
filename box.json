{
    "algorithm": "SHA256",
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "directories": [
        "src"
    ],
    "finder": [
        {
            "name": "*.php",
            "in": "vendor/",
            "exclude": [
                "bin",
                "tests",
                "Tester",
                "Tests",
                "DependencyInjection",
                "CommandLoader",
                "Logger",
                "Event",
                "EventListener"
            ]
        }
    ],
    "main": "bin/fcgi-client",
    "output": "build/fcgi-client.phar",
    "chmod": "0755",
    "git-version": "git-version",
    "stub": true
}
