{
    "name": "nir-/fcgi-client",
    "description": "CLI client for interacting with FCGI servers",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "hollodotme/fast-cgi-client": "^1.4",
        "symfony/console": "^3.4"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Albin Kerouanton",
            "email": "albin.kerouanton@knplabs.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {"NiR\\FCGIClient\\": "src/"}
    },
    "require-dev": {
        "phpunit/phpunit": "~6"
    },
    "scripts": {
        "build": [
            "rm -rf vendor/",
            "composer install --prefer-dist --no-dev",
            "box build -vvv"
        ]
    }
}
